{
	"$schema": "../gen/schemas/mobile-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": ["main"],
	"platforms": ["android"],
	"permissions": [
		"core:default",
		"shell:allow-open",
		"dialog:default",
		"dialog:allow-open",
		{
			"identifier": "fs:scope",
			"allow": [{ "path": "**" }]
		}
	]
}